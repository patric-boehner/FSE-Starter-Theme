{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"customSpacingSize": false,
			"blockGap": true,
			"padding": true,
			"margin": true,
			"units": [ "%", "px", "em", "rem", "vh", "vw" ],
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			]
		},
		"color": {
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"palette": [
				{
				"slug": "base",
				"color": "#ffffff",
				"name": "Base"
				},
				{
				"slug": "contrast",
				"color": "#151515",
				"name": "Contrast"
				},
				{
				"slug": "contrast-light",
				"color": "#666666",
				"name": "Contrast Light"
				},
				{
				"slug": "primary",
				"color": "#16768f",
				"name": "Primary"
				},
				{
				"slug": "primary-dark",
				"color": "#0f5469",
				"name": "Primary Dark"
				},
				{
				"slug": "primary-light",
				"color": "#CDE9F1",
				"name": "Primary Light"
				},
				{
				"slug": "accent",
				"color": "#83183A",
				"name": "Accent"
				},
				{
				"slug": "secondary",
				"color": "#f4a261",
				"name": "Secondary"
				},
				{
				"slug": "tertiary",
				"color": "#f5f5f5",
				"name": "Tertiary"
				},
				{
				"slug": "border-light",
				"color": "#e0e0e0",
				"name": "Border Light"
				},
				{
				"slug": "border-dark",
				"color": "#4a4a4a",
				"name": "Border Dark"
				}
			],
			"duotone": [
				{
					"colors": [
						"#000000",
						"#ffffff"
					],
					"name": "Black and White",
					"slug": "black-and-white"
				}
			],
			"gradients": [
				{
					"name": "Primary",
					"slug": "primary",
					"gradient": "linear-gradient(135deg, #16768f, #5d9eb1)"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"textTransform": false,
			"writingMode": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Mona Sans, sans-serif",
					"name": "Mona Sans",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mona Sans",
							"fontStretch": "75% 125%",
							"fontStyle": "normal",
							"fontWeight": "300 900",
							"src": [ "file:./build/fonts/mona-sans/Mona-Sans.woff2"]
						}
					]
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System",
					"slug": "system"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": ".825rem",
						"max": ".95rem"
					},
					"size": ".95rem",
					"slug": "x-small",
					"name": "Extra Small"
				},
				{
					"fluid": {
						"min": ".9rem",
						"max": "1.05rem"
					},
					"size": "1.05rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.165rem"
					},
					"size": "1.165rem",
					"slug": "base",
					"name": "Base"
				},
				{
					"fluid": {
						"min": "1.2rem",
						"max": "1.65rem"
					},
					"size": "1.65rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "2.75rem"
					},
					"size": "2.75rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": {
						"min": "1.875rem",
						"max": "3.5rem"
					},
					"size": "3.5rem",
					"slug": "x-large",
					"name": "Extra Large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "4.3875rem"
					},
					"size": "4.3875rem",
					"slug": "xx-large",
					"name": "Extra Extra Large"
				}
			]
		},
		"position": {
			"sticky": true
		},
		"shadow": {
			"defaultPresets": false,
			"presets": []
		},
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"custom": {
			"font-weight": {
				"regular": 430,
				"semi-bold": 600,
				"bold": 700
			},
			"line-height": {
				"none": 1,
				"heading": 1.1,
				"snug": 1.2,
				"body": 1.5,
				"relaxed": 1.625
			},
			"letter-spacing": {
				"none": "normal",
				"tight": "-.01em",
				"loose": ".05em",
				"looser": ".1em"
			},
			"transition": {
				"default": "all 0.2s ease-in-out"
			},
			"spacing": {
				"gap": "1.875rem"
			},
			"icon": {
				"utility": "",
				"small": "1.2rem",
				"medium": "1.8rem",
				"large": "3.2rem"
			},
			"animation": {
				"hover": "all .25s ease-in-out",
				"focus": "outline-offset .25s ease"
			}
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSizes": [],
					"fontFamilies": []
				}
			},
			"core/heading": {
				"typography": {
					"fontFamilies": []
				}
			},
			"core/group": {
				"typography": {
					"fontSizes": [],
					"fontFamilies": []
				}
			},
			"core/cover": {
				"typography": {
					"fontSizes": [],
					"fontFamilies": []
				}
			},
			"core/buttons": {
				"typography": {
					"fontSizes": [],
					"fontFamilies": []
				}
			},
			"core/button": {
				"typography": {
					"fontSizes": [],
					"fontFamilies": []
				}
			},
			"core/quote": {
				"typography": {
					"fontSizes": [],
					"fontFamilies": []
				},
				"background": {
					"backgroundImage": false
				},
				"dimensions": {
					"minHeight": false
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--line-height--snug)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--line-height--snug)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus-visible": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			},
			"button": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"text": "var(--wp--preset--color--base)",
					"background": "var(--wp--preset--color--accent)"
				},
				"border": {
					"radius": "0"
				},
				"spacing": {
					"padding": {
						"top": ".6em",
						"right": "1em",
						"bottom": ".6em",
						"left": "1em"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)",
						"background": "var(--wp--preset--color--tertiary)"
					}
				},
				":focus-visible": {
					"color": {
						"text": "var(--wp--preset--color--accent)",
						"background": "var(--wp--preset--color--tertiary)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--small)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--medium)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--medium)"
			}
		},
		"blocks": {}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "Header"
		},
		{
			"area": "footer",
			"name": "Footer"
		},
		{
			"area": "sidebar",
			"name": "Sidebar"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page"
			],
			"title": "Blank"
		},
		{
			"name": "no-title",
			"postTypes": [
				"page"
			],
			"title": "No Title"
		},
		{
			"name": "post-with-sidebar",
			"postTypes": [
				"post"
			],
			"title": "With Sidebar"
		}
	]
}